pipeline
{
    agent {
        label 'any'
    }
tools {
    maven 'myMaven'
}
    stages {
        stage('Build')
            {
                steps{
                    echo " This is build stage"
                    sh 'mvn clean package'
                }
            post {
                success {
                    archiveArtifacts artifacts: '**/*.war'
            }
            }
            }
 
    }
}
